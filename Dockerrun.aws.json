    {
      "name": "sky",
      "image": "lukk17/sky",
      "hostname": "sky",
      "essential": true,
      "memoryReservation": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ]
    }
  ]
}